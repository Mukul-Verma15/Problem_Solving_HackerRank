void plusMinus(vector<int> arr) {
     float pos=0;
     float neg=0;
     float zero=0;
     int n=arr.size();
     for(int i=0;i<arr.size();i++){
           if(arr[i]>0)
           {
               pos++;
           }
           else if(arr[i]==0)
           {
               zero++;
           }
           else {
                neg++;
           }  
     }
         if(pos+neg+zero==n)
         {
             cout<<pos/n<<endl<<neg/n<<endl<<zero/n;
         }
}
