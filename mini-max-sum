  long maxi=arr[0];
     long mini=arr[0];
     long sum=0;
     long size=arr.size();
     for(int i=0;i<size;i++){
         maxi=max(maxi,long(arr[i]));
         mini=min(mini,long(arr[i]));
     }
     for(int i=0;i<=size;i++){
         sum+=arr[i];
     }
     cout<<sum-maxi<<" "<<sum-mini<<endl;
}
